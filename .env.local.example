# Local development example envs
# Copy this file to .env.local and fill real values

# Public site URL used by client auth during SSR
NEXT_PUBLIC_SITE_URL=http://localhost:3000

# Auth
BETTER_AUTH_SECRET=replace-with-strong-random-secret

# Turso / libsql database
TURSO_CONNECTION_URL=libsql://your-db-url.turso.io
TURSO_AUTH_TOKEN=replace-with-turso-auth-token

# Blockchain / RPC (read-only public client)
BLOCKCHAIN_RPC_URL=https://rpc.ankr.com/eth

# IPFS via Web3.Storage
WEB3_STORAGE_TOKEN=replace-with-web3-storage-token
